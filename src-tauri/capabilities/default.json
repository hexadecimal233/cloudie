{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*.soundcloud.com"
        }
      ],
      "deny": []
    },
    "dialog:default"
  ]
}