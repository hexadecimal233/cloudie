  {
    "label": "Tauri Production Debug",
    "build": {
      "command": "cargo",
      "args": ["build", "--release", "--manifest-path=./src-tauri/Cargo.toml"]
    },
    "program": "$ZED_WORKTREE_ROOT/src-tauri/target/release/cloudie.exe",
    "sourceLanguages": ["rust"],
    "request": "launch",
    "adapter": "CodeLLDB"
  }
]
